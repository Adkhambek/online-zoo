{
    "name": "online-zoo",
    "version": "1.0.0",
    "description": "Online-zoo is a platform that contains information about animals from various zoos with webcams.",
    "main": "index.js",
    "scripts": {
        "watch:sass": "node-sass assets/sass/main.scss pages/donate/style.css -w",
        "server": "live-server",
        "start": "npm-run-all --parallel watch:sass server",
        "compile:sass": "node-sass assets/sass/main.scss pages/donate/style.css",
        "prefix": "postcss --use autoprefixer -b 'last 10 versions' pages/donate/style.css -o pages/donate/style.prefix.css",
        "compress": "node-sass pages/donate/style.prefix.css pages/donate/style.min.css --output-style compressed",
        "build": "npm-run-all compile:sass prefix compress"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.4.13",
        "node-sass": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^10.0.0"
    }
}
